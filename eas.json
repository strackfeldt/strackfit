            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "resourceClass": "m-medium"
            }
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview",
            "ios": {
                "resourceClass": "m-medium"
            },
            "env": {
                "EXPO_PUBLIC_BACKEND_URL": "https://strackfit-pb.tstr.app/"
            }
        },
        "production": {
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
